<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CreateAccountScreen"></activity>
        <activity android:name=".HelpfulLinksScreen"></activity>
        <activity android:name=".Journal"></activity>
        <activity android:name=".HomeScreen"></activity>
        <activity android:name=".LoginScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecoverAccountScreen"></activity>
        <activity android:name=".SafetyPlanScreen"></activity>
        <activity android:name=".SeverityTestScreen"></activity>
        <activity android:name=".VideoChatScreen"></activity>
    </application>

</manifest>